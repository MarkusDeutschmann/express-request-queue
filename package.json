{
  "name": "express-request-queue",
  "version": "1.0.4",
  "description": "Make synchronous calls to your express routes.",
  "repository": "https://github.com/jeescu/express-sync-middleware",
  "author": "John Edward <johnedwardescuyos@gmail.com>",
  "license": "MIT",
  "main": "src/index.js",
  "engines": {
    "node": "~6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r @babel/register\"",
    "clean": "npx del-cli --force dist",
    "build": "npm run clean && npx make-dir-cli dist && babel src --copy-files --out-dir dist --source-maps true",
    "test": "jest --watch",
    "lint": "esw --watch src test"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [],
  "dependencies": {
    "@babel/cli": "7.15.4",
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "7.14.7",
    "@babel/preset-env": "7.15.4",
    "body-parser": "1.19.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.15.4",
    "@babel/register": "7.15.3",
    "babel-jest": "27.1.0",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-watch": "7.0.0",
    "jest": "27.1.0",
    "nodemon": "2.0.12",
    "supertest": "6.1.6"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
